{
    "name": "{{projectName}}",
    "version": "1.0.0",
    "description": "A Bun + Express + Drizzle ORM template project",
    "module": "index.ts",
    "type": "module",
    "private": true,
    "scripts": {
        "test": "bun test",
        "dev": "bun --hot src/server.ts",
        "start": "bun build/server.js",
        "build": "bunx tsc -p tsconfig.json",
        "db:generate": "bunx drizzle-kit generate",
        "db:migrate": "bunx drizzle-kit migrate",
        "db:push": "bunx drizzle-kit push",
        "db:reset": "bunx drizzle-kit drop && drizzle-kit migrate",
        "db:studio": "bunx drizzle-kit studio",
        "swagger:generate": "bun ./src/swagger.ts"
    },
    "devDependencies": {
        "@types/bcrypt": "^6.0.0",
        "@types/bun": "latest",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.3",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/multer": "^2.0.0",
        "@types/swagger-jsdoc": "^6.0.4",
        "dotenv": "^17.2.2",
        "drizzle-kit": "^0.31.5"
    },
    "peerDependencies": {
        "typescript": "^5"
    },
    "dependencies": {
        "@neondatabase/serverless": "^1.0.1",
        "axios": "^1.12.2",
        "bcryptjs": "^3.0.2",
        "cors": "^2.8.5",
        "drizzle-orm": "^0.44.7",
        "express": "^5.1.0",
        "express-rate-limit": "^8.1.0",
        "express-validator": "^7.2.1",
        "jsonwebtoken": "^9.0.2",
        "multer": "^2.0.2",
        "openai": "^6.7.0",
        "pg": "^8.16.3",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1"
    }
}